<actions>    
	<action name="I18N_OPENXPKI_WF_ACTION_PASSWORD_SAFE_STORE_PASSWORD"
          class="OpenXPKI::Server::Workflow::Activity::PasswordSafe::StorePassword">
          <field name="_input_data"/>
          <validator name="I18N_OPENXPKI_WF_VAL_UNUSEDID"/>
    </action>
    <action name="I18N_OPENXPKI_WF_ACTION_PASSWORD_SAFE_RETRIEVE_PASSWORD"
            class="OpenXPKI::Server::Workflow::Activity::PasswordSafe::RetrievePassword">
          <field name="_id"/>
    </action>
    <action name="I18N_OPENXPKI_WF_ACTION_PASSWORD_SAFE_CHANGE_PASSWORD"
            class="OpenXPKI::Server::Workflow::Activity::PasswordSafe::StorePassword">
          <field name="_input_data"/>
    </action>
</actions>
